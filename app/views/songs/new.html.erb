<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
<label>song title</label><br>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>

  <label>artist name</label><br>
  <%= f.text_field :artist_name%><br>

  <%= f.collection_select(:genre_id, @genres, :id, :name)%>
  <input type="text" name="song[note_contents][]" id="song_notes_1">
  <input type="text" name="song[note_contents][]" id="song_notes_2">
  <input type="text" name="song[note_contents][]" id="song_notes_3">
  <%= f.submit %>	  
  <% end %>
